{
  "name": "create-a-npm",
  "type": "module",
  "version": "1.0.3",
  "description": "一个一键生成一个符合特定规则 <就是一个简单的代码模板库> 的 npm 包",
  "scripts": {
    "beautify": "npm run prettier",
    "b": "rollup --config rollup.config.js",
    "build": "jja cls rm dist && npm run b && node ./scripts/clean-package-json.js",
    "diff": "jja pkg --diff",
    "prettier": "prettier . --write",
    "eslint": "eslint src",
    "dev": "jja rm dist eg && npm run b && create_a_npm_dev=all node --trace-warnings  dist/bin.mjs -eg",
    "push:version": "gvv",
    "push:submit": "gvv",
    "vjj": "vjj",
    "prepublishOnly": "pjj",
    "prepare": "husky"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@qqi/check-version": "^1.0.3",
    "@qqi/rollup-external": "^1.0.8",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.3",
    "@types/node": "^24.0.3",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^51.1.1",
    "globals": "^16.2.0",
    "gvv": "^0.1.0",
    "husky": "^9.1.7",
    "jja": "^2.3.10",
    "lint-staged": "^16.1.2",
    "pjj": "^1.0.2",
    "prettier": "^3.5.3",
    "rollup": "^4.44.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vjj": "^1.0.6"
  },
  "dependencies": {
    "@color-pen/static": "^1.0.0",
    "@qqi/log": "^0.0.1",
    "a-command": "^2.3.10",
    "a-js-tools": "^1.0.0",
    "a-node-tools": "^4.2.12",
    "a-type-of-js": "^1.0.1",
    "color-pen": "^2.0.10",
    "colored-table": "^0.0.2",
    "qqi": "^0.0.5"
  }
}
